#include <iostream>

using namespace std;

int main() {

	int num;	int do,q,di,n,p;

	

	cout<<"Enter amount: ";

	cin>>num;

	

	do=num/100;

	num=num-d*100;

	q=num/25;

	num=num-q*25;

	di=num/10;

	num=num-di*10;

	n=num/5;

	num=num-n*5;

	p=num;

	

	cout<<"Dollar is: "<<do<<endl;

	cout<<"Quarter is: "<<q<<endl;

	cout<<"Dime is: "<<di<<endl;

	cout<<"Nickle is: "<<n<<endl;

	cout<<"Penny is: "<<p<<endl;

	return 0;

}
